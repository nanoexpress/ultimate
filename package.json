{
  "name": "@nanoexpress/ultimate",
  "type": "module",
  "version": "1.1.0",
  "description": "Ultimate solution based on nanoexpress and best-practices",
  "main": "cjs/nanoexpress.js",
  "module": "esm/nanoexpress.js",
  "typings": "typings/nanoexpress.d.ts",
  "scripts": {
    "lint": "smartlint",
    "test": "node --experimental-modules tests/index.js",
    "cleanup": "rm -rf cjs/*.js cjs/*.js.map esm/*.js esm/*.js.map typings/",
    "build": "npm run cleanup && rollup -c",
    "prepare": "husky install",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": ">=14.18.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanoexpress/ultimate.git"
  },
  "keywords": [
    "nano",
    "node",
    "fast",
    "uws",
    "websocket",
    "http",
    "c++"
  ],
  "author": "dalisoft",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nanoexpress/ultimate/issues"
  },
  "homepage": "https://github.com/nanoexpress/ultimate#readme",
  "dependencies": {
    "@nanoexpress/route-syntax-parser": "https://github.com/nanoexpress/route-syntax-parser/releases/download/v2.4.3/nanoexpress-route-syntax-parser-2.4.3.tgz",
    "@types/debug": "^4.1.7",
    "debug": "^4.3.2",
    "fast-decode-uri-component": "^1.0.1",
    "fast-query-parse": "^2.0.0",
    "path-to-regexp": "^6.2.0",
    "supports-color": "^9.0.2",
    "uWebSockets.js": "https://github.com/uNetworking/uWebSockets.js/archive/v20.2.0.tar.gz"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^16.11.6",
    "baretest": "^2.x",
    "commitlint-config-airlight": "^3.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.1.0",
    "eslint-config-airlight-node": "^3.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "lint-staged-config-airlight": "^3.0.0",
    "prettier": "^2.4.1",
    "prettier-config-airlight": "^1.0.1",
    "rollup": "^2.59.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "smartlint": "^5.0.0",
    "typedoc": "^0.22.8",
    "typescript": "^4.4.4"
  }
}
