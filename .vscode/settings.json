{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.validate": [
    "javascript"
  ],
  "typescript.validate.enable": true,
  "prettier.requireConfig": true,
  "files.associations": {
    "*.js": "javascript",
    "*.d.ts": "typescript"
  },
  "cSpell.ignorePaths": [
    "package.json",
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders"
  ],
  "cSpell.ignoreWords": [
    "nanoexpress"
  ]
}
